<div class="w-50">
    <table class="table table-hover">
        <thead>
            <tr>
                <th scope="col">Field</th>
                <th scope="col">Value</th>
            </tr>
        </thead>
        <tbody>
            <% Object.entries(data.configuration).forEach(([key, value])=> { %>
                <tr>
                    <td>
                        <%= key %>
                    </td>
                    <td>
                        <%= value %>
                    </td>
                </tr>
                <% }); %>
        </tbody>
    </table>
</div>